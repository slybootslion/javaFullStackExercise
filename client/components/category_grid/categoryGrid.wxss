/* components/category_grid/categoryGrid.wxss */

.container {
  width: 100%;
  height: 320rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.grid {
  width: 730rpx;
  height: 300rpx;
  border: 1px dashed #dcebe6;
}

.grid-item {
  width: 200rpx;
}

.img {
  width: 60rpx;
  height: 60rpx;
}

.title {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #333;
}
